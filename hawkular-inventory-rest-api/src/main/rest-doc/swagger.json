        "summary" : "Deletes an inventory entity on the given location.",
        "description" : "",
        "operationId" : "delete",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "path",
          "in" : "path",
          "required" : true,
          "type": "string",
          "description" : "See the documentation above for the format of the path."
        }, {
          "in" : "query",
          "name" : "at",
